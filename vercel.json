{
  "version": 2,
  "name": "LStudioX Moon Script",
  "public": true,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/public/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/version",
      "destination": "/api/version.json"
    }
  ],
  "redirects": [
    {
      "source": "/discord",
      "destination": "https://discord.gg/XEdgxVPnd",
      "permanent": false
    },
    {
      "source": "/github",
      "destination": "https://github.com/lstudiox/moon-script",
      "permanent": false
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true,
  "framework": "static"
}
